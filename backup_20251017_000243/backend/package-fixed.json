{
  "name": "ai-customer-service-chatbot",
  "version": "3.0.0",
  "description": "Advanced AI Customer Service Assistant with NLU, sentiment analysis, WebSocket, and real-time chat",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "start:advanced": "node src/advancedServer.js",
    "start:portfolio": "node src/portfolioServer.js",
    "dev": "nodemon --watch src -e js,json --exec node src/server.js",
    "dev:advanced": "nodemon --watch src -e js,json --exec node src/advancedServer.js",
    "dev:portfolio": "nodemon --watch src -e js,json --exec node src/portfolioServer.js",
    "test": "jest --runInBand",
    "test:coverage": "jest --coverage --runInBand",
    "test:watch": "jest --watch --runInBand",
    "lint": "eslint src/",
    "docs": "jsdoc src/ -d docs/"
  },
  "keywords": [
    "ai",
    "chatbot",
    "nlu",
    "customer-service",
    "sentiment-analysis",
    "real-time-chat",
    "websocket",
    "portfolio"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "body-parser": "^1.20.2",
    "uuid": "^9.0.0",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "jest": "^29.6.0",
    "supertest": "^6.3.4",
    "nodemon": "^3.0.0",
    "eslint": "^8.50.0",
    "jsdoc": "^4.0.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
